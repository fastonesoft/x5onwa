@import "./pages/weui.wxss";
page { background: #F6F6F6; }

.qrcode-area {
  display: flex;
  margin-top: 50rpx;
  align-items: center;
  align-content: flex-start;
  justify-content: center;
  flex-flow: column wrap;
  }
.qrcode { width: 400rpx; height: 400rpx; }

.x5ui-column { display: flex; flex-flow: row wrap; }
.x5ui-mini { text-align: center; }
.x5ui-mini button { margin-top: 0rpx; margin-right: 20rpx; }
.x5ui-mini button:last-child { margin-right: 0rpx; }
.x5ui-bottom { margin-bottom: 40rpx; }

/*增加的删除图标*/
.remove-icon { height: 50rpx; width: 50rpx; border: none; vertical-align: middle; }
.students_images { width: 46rpx; height: 46rpx; border: none; vertical-align: middle; margin-right: 15rpx; }

/* weui */
.weui-input { text-align: right; }
.weui-select { text-align: right; }
.weui-cell_vcode .weui-input { padding-right: 20rpx; }
