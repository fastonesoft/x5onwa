<!--coms/comLists.wxml-->
<block wx:for="{{lists}}" wx:key="" wx:for-index="index">
    <comList title="{{title}}" notitle="{{notitle}}" notitletext="{{index}}" obj="{{item}}" mes="{{mes}}" />
    <view class="weui-cells__tips x5ui-mini" wx:if="{{detail}}">
        <button class="weui-btn mini-btn" type="primary" size="mini" data-uid="{{item[selectKey]}}"
            bindtap="detailClick">详细</button>
    </view>
</block>
<view class="weui-cells__tips" wx:if="{{lists.length>0 && tips}}">{{tips}}</view>

