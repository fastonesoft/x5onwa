<!--coms/comList.wxml-->
<block wx:if="{{list.length>0}}">
    <view class="weui-cells__title" wx:if="{{!notitle}}">
        <block wx:if="{{!notitletext}}">{{title}}</block>
    </view>
    <view class="weui-cells weui-cells_after-title">
        <view class="weui-cell" wx:for="{{list}}" wx:key="name">
            <view class="weui-cell__bd">{{item.name}}</view>
            <view class="weui-cell__ft">{{item.value}}</view>
        </view>
    </view>
</block>

