<!--coms/comList.wxml-->
<block wx:if="{{list.length>0}}">
    <view class="weui-cells__title" wx:if="{{!notitle||notitletext}}">
        <block wx:if="{{!notitle&&!notitletext}}">{{title}}</block>
    </view>
    <view class="weui-cells weui-cells_after-title">
        <view class="weui-cell" wx:for="{{list}}" wx:key="name">
            <view class="weui-cell__bd">{{item.name}}</view>
            <view class="weui-cell__ft">
                <block wx:if="{{!item.type}}">{{item.value}}</block>
                <block wx:else>
                    <icon type="circle" size="23" wx:if="{{!item.value}}"></icon>
                    <icon type="success" size="23" wx:if="{{item.value}}"></icon>
                </block>
            </view>
        </view>
    </view>
</block>

