/**index.wxss**/
page {
    background: #F6F6F6;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}
.container{
    background: #FFF;
    border: 1px solid rgba(0, 0, 0, .1);
    border-left: none;
    border-right: none;
    padding-bottom: 0;
    background-repeat: no-repeat;
    background-size: 100% auto;
    background-position: bottom center;
    margin: 40rpx 0rpx 0rpx 0rpx;
}
/**功能导航**/
.main-swpier{
    padding: 0rpx;
}
.main-flex{
    display: flex;
    flex-flow: row wrap;
}
.main-core{
    width: 25%;
}
.main-core-item{
    display: flex;
    flex-flow: column wrap;
    height: 240rpx;
    align-items: center;
    align-content: center;
    justify-content: center;
    font-size: 30rpx;
}
.main-core-item.disabled{
    color: #9c9c9c;
}
.core-item-icon{
    width: 85rpx;
    height: 85rpx;
    margin-bottom: 25rpx;
}
