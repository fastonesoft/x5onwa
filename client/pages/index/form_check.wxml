<!--pages/index/form_check.wxml-->
<view class="weui-toptips weui-toptips_warn" wx:if="{{errorShow}}">{{errorMessage}}</view>

<comCheck url="{{url_q}}" data="{{data_q}}" checks="{{checks}}" title="{{title}}" key="{{key}}" split="{{split}}"
    memo="{{memo}}" checked="{{checked}}" bind:checkChange="checkChange" />

<view class="weui-btn-area">
    <button type="primary" bindtap="updateClick">提交</button>
</view>